#ifndef MODBUS_H_
#define MODBUS_H_

/*< modbus 功能码*/
enum Modbus_Case{
  CASE_UPDATE=0,		// 固件程序更新
  CASE_MODULE_RESPON,		// 总线占用申请
  CASE_MODULE_MSG,		// 设备信息查询
  CASE_MODULE_SET_NET_ID,	// 设置设备网络ID,组ID
  CASE_CHANGE_PORT, 		// 设备功能，网关主动发送更新数据
  CASE_Read_PORT,		//设备功能，读取输出状态
};

struct Modbus_Protocol

#endif /* MODBUS_H_ */
